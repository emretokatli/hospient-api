{
  "version": 2,
  "builds": [
    {
      "src": "api/simple.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/simple.js": {
      "maxDuration": 30
    }
  }
} 